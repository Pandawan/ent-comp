{
  "name": "ent-comp",
  "version": "1.2.1",
  "description": "A light, fast Entity Component System in TS",
  "main": "dist/ECS.js",
  "types": "dist/ECS.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "tape test/*.js",
    "bench": "node bench/suite.js",
    "liveBench": "(cd bench; webpack-dev-server)",
    "doc": "typedoc src --out docs --theme markdown --readme none --mode file --excludeNotExported"
  },
  "keywords": [
    "ECS",
    "ts",
    "Entity",
    "Control",
    "System"
  ],
  "authors": [
    "Andy Hall",
    "Pandawan"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyhall/ent-comp"
  },
  "devDependencies": {
    "fen-bench": "^0.1.0",
    "tape": "^4.4.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.1.3",
    "typescript": "^3.5.3"
  },
  "dependencies": {}
}
